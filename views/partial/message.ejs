<%- include('header')-%> <% if (typeof errors !='undefined'){%> <%
errors.forEach(element => { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= element.msg %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% }); } %> <%-include('footer')-%>
